//---------------------  ******** 5.9	Pearson Correlation coefficient feature selection (pearsonr) ********--------------------------
python feaSelector.py --file my_codes/features/iFeature/round2/AAC/AAC_100_mixed_train.tsv --type pearsonr --label my_codes/data/mixed_data/label/train_mixed_100_label.txt --out my_codes/features/iFeature/featureSelection/pearsonr/AAC/pearsonr_AAC_100_mixed_train.txt

