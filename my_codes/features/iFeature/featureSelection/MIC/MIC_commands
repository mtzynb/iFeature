//---------------------  ******** 5.8	Mutual Information feature selection (MIC) ********--------------------------
python feaSelector.py --file my_codes/features/iFeature/round2/AAC/AAC_100_mixed_train.tsv --type MIC --label my_codes/data/mixed_data/label/train_mixed_100_label.txt --out my_codes/features/iFeature/featureSelection/MIC/AAC/MIC_AAC_100_mixed_train.txt

//---------------------------------------------------
python feaSelector.py --file my_codes/data/mixed_data/train_mixed_90.fasta --type MIC --out my_codes/features/iFeature/round2/MIC/MIC_90_mixed_train.tsv

//---------------------------------------------------
python feaSelector.py --file my_codes/data/mixed_data/train_mixed_80.fasta --type MIC --out my_codes/features/iFeature/round2/MIC/MIC_80_mixed_train.tsv

