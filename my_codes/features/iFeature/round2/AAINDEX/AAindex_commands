//---------------------  ******** AAINDEX  ********--------------------------
Error: for "AAINDEX" encoding, the input fasta sequences should be with equal length.
//---------------------------------------------------



python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type AAINDEX --out my_codes/features/iFeature/AAINDEX/AAINDEX_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type AAINDEX --out my_codes/features/iFeature/AAINDEX/AAINDEX_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type AAINDEX --out my_codes/features/iFeature/AAINDEX/AAINDEX_100_mixed.tsv

