//---------------------  ******** Quasi-sequence-order (QSOrder) ********--------------------------
python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type QSOrder --out my_codes/features/iFeature/QSOrder/QSOrder_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type QSOrder --out my_codes/features/iFeature/QSOrder/QSOrder_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type QSOrder --out my_codes/features/iFeature/QSOrder/QSOrder_100_mixed.tsv

