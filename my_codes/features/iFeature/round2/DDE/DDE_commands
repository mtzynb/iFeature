//---------------------  ******** Dipeptide Deviation from Expected Mean (DDE) ********--------------------------

python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type DDE --out my_codes/features/iFeature/DDE/DDE_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type DDE --out my_codes/features/iFeature/DDE/DDE_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type DDE --out my_codes/features/iFeature/DDE/DDE_100_mixed.tsv

//---------------------------------------------------
