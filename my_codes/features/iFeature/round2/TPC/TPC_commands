//---------------------  ******** Tri-Peptide Composition (TPC) ********--------------------------

python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type TPC --out my_codes/features/iFeature/TPC/TPC_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type TPC --out my_codes/features/iFeature/TPC/TPC_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type TPC --out my_codes/features/iFeature/TPC/TPC_100_mixed.tsv

