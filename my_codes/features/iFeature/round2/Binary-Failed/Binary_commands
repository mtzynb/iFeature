//---------------------  ******** Binary profiling ********--------------------------
python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type BINARY --out my_codes/features/iFeature/BINARY/BINARY_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type BINARY --out my_codes/features/iFeature/BINARY/BINARY_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type BINARY --out my_codes/features/iFeature/BINARY/BINARY_100_mixed.tsv

//---------------------------------------------------
