//---------------------  ******** (Conjoint triad CTriad) ********--------------------------
python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type CTriad --out my_codes/features/iFeature/CTriad/CTriad_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type CTriad --out my_codes/features/iFeature/CTriad/CTriad_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type CTriad --out my_codes/features/iFeature/CTriad/CTriad_100_mixed.tsv

