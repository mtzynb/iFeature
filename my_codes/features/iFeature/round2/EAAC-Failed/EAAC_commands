//---------------------  ******** EAAC with default window size = 5 ********--------------------------
Error: for "EAAC" encoding, the input fasta sequences should be with equal length.

python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type EAAC --out my_codes/features/iFeature/EAAC/EAAC_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type EAAC --out my_codes/features/iFeature/EAAC/EAAC_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type EAAC --out my_codes/features/iFeature/EAAC/EAAC_100_mixed.tsv

