//---------------------  ******** Grouped Amino Acid Composition (GAAC) ********--------------------------

python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type GAAC --out my_codes/features/iFeature/GAAC/GAAC_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type GAAC --out my_codes/features/iFeature/GAAC/GAAC_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type GAAC --out my_codes/features/iFeature/GAAC/GAAC_100_mixed.tsv

//---------------------------------------------------
