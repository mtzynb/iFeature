//---------------------  ******** Grouped Di-Peptide Composition (GDPC): total size: 25 descriptors  ********--------------------------
python iFeature.py --file my_codes/split_data/ACP_100_pos.fasta --type GDPC --out my_codes/features/iFeature/GDPC/GDPC_100_pos.tsv

python iFeature.py --file my_codes/split_data/ACP_100_neg.fasta --type GDPC --out my_codes/features/iFeature/GDPC/GDPC_100_neg.tsv

python iFeature.py --file my_codes/data/ACP_dataset/fasta/ACP-Mixed-100.fasta --type GDPC --out my_codes/features/iFeature/GDPC/GDPC_100_mixed.tsv

